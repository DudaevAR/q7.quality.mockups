Content-Type: text/ecl
Entry-Name: .content

get-view "Q7 Quality Mockups" | get-tree | select "swt/expandBar/ExpandBar"
with [get-view "Q7 Quality Mockups" | get-group ExpandBar] {
    with [get-control Any -index 4] {
        get-property className | equals "org.eclipse.swt.widgets.ExpandBar" | verify-true
        get-property enablement | equals true | verify-true
        get-property "getItems().ExpandItem[0].getClassName()" | equals "org.eclipse.swt.widgets.ExpandItem" 
            | verify-true
        get-property "getItems().ExpandItem[0].getControl().getText()" 
            | equals "initial text that will wrap if it\'s long enough" | verify-true
        get-property "getItems().ExpandItem[0].getText()" | equals "item 1" | verify-true
        get-property "getItems().ExpandItem[1].getControl().getText()" | equals "This is a text of Expand Item 2" 
            | verify-true
        get-property "getItems().ExpandItem[1].getText()" | equals "item 2" | verify-true
    }
    get-editbox -index 1 | get-property text | equals "This is a text of Expand Item 2" | verify-true
    get-editbox | get-property text | equals "initial text that will wrap if it\'s long enough" | verify-true
}
with [get-view "Q7 Quality Mockups" | get-group ExpandBar | get-control Any -index 4] {
    get-property "getItems().ExpandItem[0].getExpanded()" | equals false | verify-true
    get-property "getItems().ExpandItem[1].getExpanded()" | equals false | verify-true
}
with [get-view "Q7 Quality Mockups" | get-group ExpandBar | get-control Any -index 4] {
    get-property "getItems().ExpandItem[0].getExpanded()" | equals true | verify-true
    get-property "getItems().ExpandItem[1].getExpanded()" | equals true | verify-true
}
